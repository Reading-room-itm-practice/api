{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "JWT": {
    "ValidAudience": "<JWT_ValidAudience>",
    "Secret": "<JWT_Secret>"
  },

  "FileSizeLimit": 10000000,
  "AllowedPhotoExtensions": ".jpeg, .jpg, .png",
  "PhotoUploadsFolder": "..\\Storage\\Photos"
  
  "SMTP": {
    "Host": "<SMTP_Host>",
    "Port": "<SMTP_Port>",
    "Username": "<SMTP_Username>",
    "Password": "<SMTP_Password>"
  },

  "ReturnPaths": {
    "ConfirmEmail": "<ReturnPaths:ConfirmEmail>",
    "SenderEmail": "<ReturnPaths:SenderEmail>"
  },

  "Admin": {
    "Password": "<Admin:Password>",
    "Username": "<Admin:Username>",
    "Email": "<Admin:Email>"
  }
} 
