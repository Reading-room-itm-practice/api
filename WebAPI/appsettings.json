{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "JWT": {
    "Secret": "<JWT_Secret>",
    "ValidAudience": "<JWT_ValidAudience>"
  },

  "FileSizeLimit": 10000000,
  "AllowedPhotoExtensions": ".jpeg, .jpg, .png",
  "PhotoUploadsFolder": "..\\Storage\\Photos",
  
  "SMTP": {
    "Host": "<SMTP_Host>",
    "Port": "<SMTP_Port>",
    "Username": "<SMTP_Username>",
    "Password": "<SMTP_Password>"
  },

  "ReturnPaths": {
    "ConfirmEmail": "<ReturnPaths:ConfirmEmail>",
    "SenderEmail": "<ReturnPaths:SenderEmail>"
  },

  "Admin": {
    "Email": "<Admin_Email>",
    "Password": "<Admin_Password>",
    "Username": "<Admin_Username>"
  },

  "Google": {
    "Id": "<Google_Id>",
    "Secret": "<Google_Secret>"
  }
} 
