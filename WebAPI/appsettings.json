{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "JWT": {
    "Secret": "<JWT_Secret>",
    "ValidAudience": "<JWT_ValidAudience>"
  },

  "SMTP": {
    "Host": "<SMTP_Host>",
    "Port": "<SMTP_Port>",
    "Username": "<SMTP_Username>",
    "Password": "<SMTP_Password>"
  },

  "ReturnPaths": {
    "ConfirmEmail": "<ReturnPaths:ConfirmEmail>",
    "SenderEmail": "<ReturnPaths:SenderEmail>"
  },

  "Admin": {
    "Email": "<Admin:Email>",
    "Password": "<Admin:Password>",
    "Username": "<Admin:Username>"
  },

  "User": {
    "PasswordExpression": "^(?=.*[0-9])(?=.*[!@#$%^&*])(?=.*[A-Z])[0-9a-zA-Z!@#$%^&*0-9]{7,}$"
  }
} 
