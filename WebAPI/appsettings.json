{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "JWT": {
    "Secret": "<JWT_Secret>",
    "ValidAudience": "<JWT_ValidAudience>"
  },

  "FileSizeLimit": 10000000,
  "AllowedPhotoExtensions": ".jpeg, .jpg, .png",
  "PhotoUploadsFolder": "..\\Storage\\Photos",

  "SMTP": {
    "Host": "<SMTP_Host>",
    "Port": "<SMTP_Port>",
    "Name": "SMTP_EmailName",
    "Username": "<SMTP_Username>",
    "Password": "<SMTP_Password>"
  },

  "Paths": {
    "ConfirmEmail": "<Paths_ConfirmEmail>",
    "ResetPassword": "<Paths_ResetPassword>"
  },

  "Admin": {
    "Email": "<Admin_Email>",
    "Password": "<Admin_Password>",
    "Username": "<Admin_Username>"
  },

  "External": {
    "RedirectUrl": "<External_RedirectUrl>"
  },

  "Google": {
    "Id": "<Google_Id>",
    "Secret": "<Google_Secret>"
  },

  "Twitter": {
    "APIKey": "<Twitter_APIKey>",
    "Secret": "<Twitter_Secret>"
  },

  "GitHub": {
    "Id": "GitHub_Id",
    "Secret": "<GitHub_Secret>"
  },

  "Facebook": {
    "AppId": "<Facebook_AppId>",
    "Secret": "Facebook_Secret"
  }
}
